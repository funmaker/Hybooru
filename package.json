{
	"name": "hybooru",
	"description": "Hydrus-based booru-styled imageboard in React",
	"version": "1.6.0",
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=7.0.0"
	},
	"main": "server.ts",
	"author": "Fun Maker",
