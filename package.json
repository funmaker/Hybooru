{
 "name": "Boilerplate",
 "description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
 "version": "1.0.0",
 "main": "./server.js",
 "author": "Fun Maker",
 "scripts": {
  "start": "cross-env NODE_ENV=development nodemon --exec babel-node --presets react,env server.js",
  "start:prod": "cd dist && cross-env NODE_ENV=production node server.bundle.js",
  "build:prod": "npm run build:prod-webpack && npm run build:prod-static",
  "build:prod-webpack": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --config webpack.config.js",
  "build:prod-static": "cp -r static dist/"
 },
 "dependencies": {
  "axios": "^0.17.1",
  "babel-polyfill": "^6.26.0",
  "better-npm-run": "^0.0.15",
  "bootstrap": "^3.3.7",
  "detect-node": "latest",
  "express": "^4.15.2",
  "express-handlebars": "^3.0.0",
  "express-promise-router": "^2.0.0",
  "express-subdomain": "^1.0.5",
  "prop-types": "^15.5.8",
  "react": "^15.5.4",
  "react-dom": "^15.5.4",
  "react-router": "^4.1.1",
  "react-router-dom": "^4.2.2",
  "webpack-node-externals": "^1.6.0"
 },
 "devDependencies": {
  "babel-cli": "^6.26.0",
  "babel-core": "^6.24.1",
  "babel-loader": "^7.0.0",
  "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
  "babel-preset-env": "^1.6.1",
  "babel-preset-es2015": "^6.24.1",
  "babel-preset-react": "^6.24.1",
  "cross-env": "^5.1.3",
  "css-loader": "^0.28.1",
  "extract-text-webpack-plugin": "^2.1.0",
  "handlebars-loader": "^1.6.0",
  "less": "^2.7.3",
  "less-loader": "^4.0.5",
  "nodemon": "^1.11.0",
  "react-hot-loader": "^1.3.1",
  "style-loader": "^0.17.0",
  "uglifyjs-webpack-plugin": "^1.1.4",
  "webpack": "^2.5.0",
  "webpack-dev-middleware": "^1.10.2",
  "webpack-dev-server": "^2.4.5",
  "webpack-hot-middleware": "^2.18.0"
 }
}
