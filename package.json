{
	"name": "Boilerplate",
	"description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
	"version": "1.1.0",
	"main": "./server.js",
	"author": "Fun Maker",
	"repository": "https://github.com/funmaker/webapp-boilerplate",
	"scripts": {
		"start": "rm -rf build && webpack --config webpack/server.dev.babel.js",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.babel.js",
		"build:prod-client": "webpack --config webpack/client.babel.js",
		"build:prod-static": "cp -r static dist/",
		"build:prod-package": "babel-node webpack/package.babel.js"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"body-parser": "latest",
		"colors": "latest",
		"compression": "^1.7.2",
		"cookie-parser": "^1.4.3",
		"detect-node": "latest",
		"express": "^4.16.3",
		"express-handlebars": "^3.0.0",
		"express-promise-router": "^3.0.2",
		"morgan": "^1.9.0",
		"prop-types": "^15.6.1",
		"query-string": "^6.1.0",
		"react": "^16.4.0",
		"react-dom": "^16.4.0",
		"react-hot-loader": "^4.2.0",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"source-map-support": "^0.5.6"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.49",
		"@babel/node": "^7.0.0-beta.49",
		"@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
		"@babel/polyfill": "^7.0.0-beta.49",
		"@babel/preset-env": "^7.0.0-beta.49",
		"@babel/preset-react": "^7.0.0-beta.49",
		"@babel/register": "^7.0.0-beta.49",
		"babel-loader": "^8.0.0-beta.3",
		"css-loader": "^0.28.11",
		"handlebars": "^4.0.11",
		"handlebars-loader": "^1.7.0",
		"mini-css-extract-plugin": "^0.4.0",
		"node-sass": "^4.9.0",
		"npm-run-all": "^4.1.3",
		"sass-loader": "^7.0.1",
		"start-server-webpack-plugin": "^2.2.5",
		"style-loader": "^0.21.0",
		"webpack": "^4.9.1",
		"webpack-cli": "^2.1.4",
		"webpack-dev-middleware": "^3.1.3",
		"webpack-hot-middleware": "^2.22.2",
		"webpack-node-externals": "^1.7.2"
	},
	"port": 3900
}
